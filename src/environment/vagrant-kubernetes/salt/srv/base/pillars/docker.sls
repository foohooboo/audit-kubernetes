docker:
  # The architecture to use for the docker install
  architecture: amd64
  # The distribution to use for the docker install
  distribution: xenial
  # The GPG key url to verify the package repository
  gpg_key_url: https://download.docker.com/linux/ubuntu/gpg
  # The package repository to use for docker
  repo_url: https://download.docker.com/linux/ubuntu
